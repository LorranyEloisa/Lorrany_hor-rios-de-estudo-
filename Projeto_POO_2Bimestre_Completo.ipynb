    "\n",
    "import mysql.connector\n",
    "\n",
    "class Estudo:\n",
    "    def __init__(self, disciplina, dia, hora):\n",
    "        self.disciplina = disciplina\n",
    "        self.dia = dia\n",
    "        self.__hora = hora\n",
    "\n",
    "    def exibir_dados(self):\n",
    "        print(f\"Disciplina: {self.disciplina}\")\n",
    "        print(f\"Dia: {self.dia}\")\n",
    "        print(f\"Hora: {self.__hora}\")\n",
    "        print(\"-\" * 30)\n",
    "\n",
    "    def set_hora(self, nova_hora):\n",
    "        if self.validar_hora(nova_hora):\n",
    "            self.__hora = nova_hora\n",
    "            print(f\"A hora foi alterada para {self.__hora}\")\n",
    "        else:\n",
    "            print(\"Hora inv√°lida. Hora n√£o alterada.\")\n",
    "\n",
    "    def validar_hora(self, hora):\n",
    "        try:\n",
    "            h, m = map(int, hora.split(\":\"))\n",
    "            return 0 <= h < 24 and 0 <= m < 60\n",
    "        except ValueError:\n",
    "            return False\n",
    "\n",
    "# -------- Banco de Dados --------\n",
    "\n",
    "def criar_conexao_db():\n",
    "    conn = mysql.connector.connect(\n",
    "        host='localhost',\n",
    "        user='root',\n",
    "        password='',  # coloque a senha do MySQL aqui\n",
    "        database='agenda_estudos'\n",
    "    )\n",
    "    return conn\n",
    "\n",
    "def inserir_estudo(conn, disciplina, dia, hora, nivel):\n",
    "    cursor = conn.cursor()\n",
    "    cursor.execute('''\n",
    "    INSERT INTO estudos (disciplina, dia, hora, nivel)\n",
    "    VALUES (%s, %s, %s, %s)\n",
    "    ''', (disciplina, dia, hora, nivel))\n",
    "    conn.commit()\n",
    "\n",
    "def consultar_estudos(conn):\n",
    "    cursor = conn.cursor()\n",
    "    cursor.execute('SELECT * FROM estudos')\n",
    "    return cursor.fetchall()\n",
    "\n",
    "def excluir_estudo(conn, estudo_id):\n",
    "    cursor = conn.cursor()\n",
    "    cursor.execute('DELETE FROM estudos WHERE id = %s', (estudo_id,))\n",
    "    conn.commit()\n",
    "\n",
    "# -------- Menu Interativo --------\n",
    "\n",
    "def menu():\n",
    "    conn = criar_conexao_db()\n",
    "    \n",
    "    while True:\n",
    "        print(\"\\n=== AGENDA DE ESTUDOS ===\")\n",
    "        print(\"1. Cadastrar Estudo\")\n",
    "        print(\"2. Consultar Estudos\")\n",
    "        print(\"3. Excluir Estudo\")\n",
    "        print(\"4. Sair\")\n",
    "        \n",
    "        opcao = input(\"Escolha uma op√ß√£o: \")\n",
    "        \n",
    "        if opcao == '1':\n",
    "            disciplina = input(\"Digite a disciplina: \")\n",
    "            dia = input(\"Digite o dia da semana: \")\n",
    "            hora = input(\"Digite a hora (HH:MM): \")\n",
    "            nivel = input(\"Digite o n√≠vel: \")\n",
    "\n",
    "            estudo = Estudo(disciplina, dia, hora)\n",
    "            if estudo.validar_hora(hora):\n",
    "                inserir_estudo(conn, disciplina, dia, hora, nivel)\n",
    "                print(\"Estudo cadastrado com sucesso!\")\n",
    "            else:\n",
    "                print(\"Hora inv√°lida. N√£o foi poss√≠vel cadastrar.\")\n",
